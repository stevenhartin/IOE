//////////////////////////////////////////////////////////////////////////
// Structures
//////////////////////////////////////////////////////////////////////////

// Input
struct VS_INPUT
{
    float4 Pos      : POSITION;
};

// Output
struct VS_OUTPUT
{
    float4 Pos		: SV_POSITION;
};

//////////////////////////////////////////////////////////////////////////

VS_OUTPUT main(VS_INPUT input)
{
    VS_OUTPUT Out = (VS_OUTPUT)0;
    Out.Pos = input.Pos;

    return Out;
}

